{
  "name": "figma-web-comparison-tool",
  "version": "1.0.0",
  "description": "A tool that compares UI designs in Figma with actual implemented web screens",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:unified": "node start-server.js unified",
    "start:main": "node start-server.js main",
    "start:legacy": "node start-server.js legacy",
    "start:enhanced": "node start-server.js enhanced",
    "dev": "concurrently \"npm run start:unified\" \"cd frontend && npm run dev\"",
    "dev:frontend": "vite",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build:netlify": "npm run build:frontend",
    "preview": "vite preview",
    "dev:legacy": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:watch": "node --watch test-comparison.js",
    "test:coverage": "node --experimental-test-coverage test-comparison.js",
    "test:unit": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --testPathPattern=tests/unit",
    "test:integration": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --testPathPattern=tests/integration",
    "test:api": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --testPathPattern=tests/api",
    "test:e2e": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --testPathPattern=tests/e2e",
    "test:performance": "NODE_OPTIONS=\"--experimental-vm-modules\" jest --testPathPattern=tests/performance",
    "compare": "node src/cli.js",
    "server:help": "node start-server.js --help",
    "ports:check": "node scripts/verify-ports.js",
    "ports:fix": "node scripts/verify-ports.js --fix",
    "start:port": "node start-server.js unified --port"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.53.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.9.0",
    "chalk": "^5.3.0",
    "chart.js": "^4.4.9",
    "commander": "^11.1.0",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "natural": "^8.1.0",
    "openai": "^5.1.1",
    "ora": "^8.0.1",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "puppeteer": "^22.15.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3",
    "serverless-http": "^3.2.0",
    "sharp": "^0.33.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "typescript": "^5.8.3",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@jest/globals": "^29.7.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.80.6",
    "@testing-library/jest-dom": "^6.1.5",
    "@types/express": "^5.0.3",
    "@types/puppeteer": "^5.4.7",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "framer-motion": "^12.16.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-puppeteer": "^9.0.2",
    "msw": "^2.0.11",
    "nock": "^13.4.0",
    "nodemon": "^3.0.2",
    "react-hook-form": "^7.57.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.5"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testTimeout": 30000
  },
  "keywords": [
    "figma",
    "design-system",
    "ui-testing",
    "visual-diff",
    "design-qa"
  ],
  "author": "AI Developer",
  "license": "MIT"
}
