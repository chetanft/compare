# Next Version Features Configuration
# Copy to .env or add to your existing .env file

# Enable Next Version Features
ENABLE_NEXT_VERSION=true

# Feature Flags (can be enabled individually)
ENABLE_VISUAL_COMPARISON=true
ENABLE_LLM_INTEGRATION=true
ENABLE_MULTI_PAGE=false
ENABLE_PERFORMANCE_DASHBOARD=false
ENABLE_ERROR_RECOVERY=true

# LLM Integration (Free Providers)
LLM_PROVIDER=deepseek
LLM_API_KEY=your_deepseek_api_key_here
LLM_API_URL=https://api.deepseek.com/v1/chat/completions
LLM_MODEL=deepseek-chat
LLM_MAX_TOKENS=1000
LLM_TIMEOUT=30000

# Authentication Retry Configuration
AUTH_MAX_RETRIES=3
AUTH_RETRY_DELAY=2000
AUTH_SELECTOR_TIMEOUT=15000
AUTH_SKIP_ON_FAILURE=true

# Visual Comparison Settings
VISUAL_DIFF_THRESHOLD=0.1
VISUAL_SIMILARITY_THRESHOLD=85.0

# Alternative LLM Providers (Qwen example)
# LLM_PROVIDER=qwen
# LLM_API_KEY=your_qwen_api_key_here
# LLM_API_URL=https://dashscope.aliyuncs.com/api/v1/services/aigc/text-generation/generation
# LLM_MODEL=qwen-turbo 